struct Person {
    name: str
    age: int
    male: bool
}

fun idk(p: Person) -> int {
    p.male <- true
    printf('%b\n', p.male)
    return p.age
}

fun main() -> int {
    p <- Person('john', 52, true)
    printf('%b\n', p.male)
    p.male <- false
    printf('%b\n', p.male)
    return idk(p)
}